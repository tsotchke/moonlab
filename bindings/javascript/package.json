{
  "name": "moonlab-javascript",
  "version": "0.1.1",
  "private": true,
  "description": "Moonlab Quantum Simulator - JavaScript/WebAssembly Bindings",
  "scripts": {
    "build": "turbo run build",
    "build:wasm": "pnpm --filter @moonlab/quantum-core build:wasm",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:unit": "pnpm --filter @moonlab/quantum-core test:unit",
    "test:integration": "pnpm --filter @moonlab/quantum-core test:integration",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "pnpm": "^10.28.0"
  }
}
