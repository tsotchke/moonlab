{
  "name": "@moonlab/quantum-react",
  "version": "1.0.0",
  "description": "React components for Moonlab quantum simulator",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.tsx",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "quantum",
    "react",
    "hooks",
    "components",
    "visualization"
  ],
  "author": "Moonlab",
  "license": "Apache-2.0",
  "dependencies": {
    "@moonlab/quantum-core": "workspace:*",
    "@moonlab/quantum-viz": "workspace:*"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "sideEffects": false
}
