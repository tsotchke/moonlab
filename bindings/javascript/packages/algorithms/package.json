{
  "name": "@moonlab/quantum-algorithms",
  "version": "0.1.1",
  "description": "Quantum algorithms for Moonlab simulator",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest run",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "quantum",
    "grover",
    "vqe",
    "qaoa",
    "algorithms"
  ],
  "author": "Moonlab",
  "license": "Apache-2.0",
  "dependencies": {
    "@moonlab/quantum-core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "vitest": "^1.0.0"
  },
  "sideEffects": false
}
